dev tunDON
client
remote YOUR_WINDMILL_PUBLIC_HOSTNAME
tls-client
verify-x509-name "YOUR WINDMILL CERTIFICATE CN"
port 1194
float
auth-user-pass /run/don/credentials
ca /usr/share/don/ca.pem
auth-nocache
compress lz4
explicit-exit-notify 1
verb 3
persist-key
persist-tun
nobind
management /run/don/management unix
